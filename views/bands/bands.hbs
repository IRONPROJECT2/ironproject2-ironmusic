      <h1>Rock</h1>
    </div>
    <div id="Rock" class="d-flex flex-wrap justify-content-center align-items-center w-100">
      {{#each bands}}
        {{#musicalMatch musicalGenre "Rock"}}
          <div class="card mx-2 mt-4" style="width: 18rem;">
            <img src="{{photo}}" class="card-img-fluid" alt="band image" style="height: 200px; object-fit: cover;">
            <div class="card-body bg-black">
              <h5 class="card-title text-center text-white">{{name}}</h5>
              <p class="d-inline-block text-truncate text-center text-white" style="max-width: 250px;">{{biography}}</p>
              <a href="/band/{{id}}/detail" class="btn btn-primary text-white">Ver más</a>
            </div>
          </div>
        {{/musicalMatch}}
      {{/each}}
    </div>
  </div>

