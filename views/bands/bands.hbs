<section class="container text-center my-5 w-50">
  <h1 class="text-center my-2">NUESTRAS BANDAS MUSICALES</h1>
</section>

<section class="mx-auto border border-white my-5 w-75">
  <div class="d-flex flex-wrap justify-content-center align-content-center">
    {{#each genre}}
      <a class="text-decoration-none fs-5 text-white me-2" href="#{{this}}"><u>{{this}}</u></a>
    {{/each}}
  </div>
</section>


